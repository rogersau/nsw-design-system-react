{
  "name": "@rogersau123/nsw-ds-react",
  "version": "1.0.2",
  "description": "Reusable React Components from the NSW Design System. Forked from ",
  "author": "James Rogers",
  "license": "MIT",
  "repository": "https://github.com/rogersau/nsw-design-system-react",
  "main": "dist/index.js",
  "module": "dist/index.modern.js",
  "types": "./dist/index.d.ts",
  "packageManager": "pnpm@10.20.0",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "source": "src/index.ts",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "dev": "storybook dev -p 6006",
    "build": "tsup",
    "build:storybook": "storybook build",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "type-check": "tsc --noEmit",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build"
  },
  "peerDependencies": {
    "react": "^16.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.28.5",
    "@babel/preset-react": "^7.28.5",
    "@babel/preset-typescript": "^7.28.5",
    "@mdx-js/loader": "^3.1.1",
    "@storybook/addon-docs": "^10.0.5",
    "@storybook/addon-links": "10.0.5",
    "@storybook/react-vite": "^10.0.5",
    "@types/node": "^24.10.0",
    "@types/prop-types": "^15.7.15",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "babel-loader": "^10.0.0",
    "cross-env": "^7.0.3",
    "gh-pages": "^3.2.3",
    "npm-run-all": "^4.1.5",
    "nsw-design-system": "^3.0.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-id-generator": "^3.0.2",
    "react-scripts": "^5.0.1",
    "storybook": "^10.0.5",
    "vite": "^7.2.0",
    "tsup": "^8.5.0",
    "typescript": "^5.9.3"
  },
  "files": [
    "dist"
  ]
}
