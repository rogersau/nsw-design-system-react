{
  "name": "@rogersau123/nsw-ds-react",
  "version": "1.0.2",
  "description": "Reusable React Components from the NSW Design System. Forked from ",
  "author": "James Rogers",
  "license": "MIT",
  "repository": "https://github.com/rogersau/nsw-design-system-react",
  "main": "dist/index.js",
  "module": "dist/index.modern.js",
  "types": "./dist/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.js",
      "types": "./dist/index.d.ts"
    }
  },
  "source": "src/index.js",
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "build": "microbundle-crl --no-compress --format modern,cjs",
    "start": "microbundle-crl watch --no-compress --format modern,cjs",
    "prepare": "run-s build",
    "test": "run-s test:unit test:lint test:build",
    "test:build": "run-s build",
    "test:lint": "eslint .",
    "test:unit": "cross-env CI=1 react-scripts test --env=jsdom",
    "test:watch": "react-scripts test --env=jsdom",
    "predeploy": "cd example && npm install && npm run build",
    "deploy-storybook": "build-storybook && gh-pages -d storybook-static",
    "storybook": "storybook dev -p 6006",
    "build:ts": "tsup",
    "dev:ts": "tsup --watch",
    "build:ts-test": "tsup src/ts-test-entry.ts --format esm,cjs --clean",
    "build:dts": "tsc -p tsconfig.types.json",
    "build-storybook": "build-storybook"
  },
  "peerDependencies": {
    "react": "^16.0.0"
  },
  "devDependencies": {
    "@babel/core": "^7.28.5",
    "@babel/preset-react": "^7.28.5",
    "@babel/preset-typescript": "^7.28.5",
    "@mdx-js/loader": "^3.1.1",
    "@storybook/addon-docs": "8.6.14",
    "@storybook/addon-essentials": "^8.6.14",
    "@storybook/addon-links": "^8.6.14",
    "@storybook/react": "^8.6.14",
    "@storybook/react-webpack5": "^8.6.14",
    "@storybook/test": "^8.6.14",
    "@types/node": "^24.10.0",
    "@types/prop-types": "^15.7.15",
    "@types/react": "^19.2.2",
    "@types/react-dom": "^19.2.2",
    "babel-loader": "^10.0.0",
    "cross-env": "^7.0.3",
    "gh-pages": "^3.2.3",
    "microbundle-crl": "^0.13.11",
    "npm-run-all": "^4.1.5",
    "nsw-design-system": "^3.0.2",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "react-id-generator": "^3.0.2",
    "react-scripts": "^5.0.1",
    "storybook": "^8.6.14",
    "tsup": "^8.5.0",
    "typescript": "^5.9.3"
  },
  "files": [
    "dist"
  ]
}
